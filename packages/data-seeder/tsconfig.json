{
  "compilerOptions": {
    "skipLibCheck": true,
    "esModuleInterop": true,
    "baseUrl": "./src",
    "paths": {
      "@data-seeder/*": ["./*"]
    },
    "target": "es6",
    "module": "node16",
    "outDir": "build/dist",
    "sourceMap": true,
    "moduleResolution": "node16",
    "rootDir": ".",
    "lib": ["esnext.asynciterable", "es6", "es2019", "DOM.Iterable"],
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "types": ["node", "fhir"]
  },
  "include": ["src/**/*.ts", "typings"]
}
