{
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@data-seeder/*": [
        "./*"
      ]
    },
    "target": "es6",
    "module": "commonjs",
    "outDir": "build/dist",
    "sourceMap": true,
    "moduleResolution": "node",
    "rootDir": ".",
    "lib": [
      "esnext.asynciterable",
      "es6",
      "es2019",
      "DOM.Iterable"
    ],
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "types": [
      "node"
    ]
  },
  "include": [
    "src/**/*.ts",
    "typings"
  ]
}
